public class Puppy extends Dog {

    public Puppy(String habitat, String name) {
        super(habitat, name);
    }

    public void bite(){
        System.out.println("ARrrrr");
    }

    public void run(){
        System.out.println("Puppy is running");
    }

    public void jump(){
        System.out.println("Puppy is jumping");
    }
}
