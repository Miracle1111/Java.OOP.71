public class Puppy extends Dog {
    String breed;

    public Puppy(String habitat, String name, String breed) {
        super(habitat, name);
        this.breed = breed;
        
    }

public String getBreed() {
    return breed;
}
    public void bite(){
        System.out.println("ARrrrr");
    }

    public void run(){
        System.out.println("Puppy is running");
    }

    public void jump(){
        System.out.println("Puppy is jumping");
    }
}
