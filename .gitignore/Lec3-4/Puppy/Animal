public class Animal {

    private String habitat;

    public Animal(String habitat) {
        this.habitat = habitat;
    }

    public String getHabitat() {
        return habitat;
    }

    public void sleep(){
        System.out.println("Sleeping...");
    }


    public void eat(){
        System.out.println("Eating");
    }

    public void breath(){
        System.out.println("Breathing...");
    }
   
}
