public class Movie {
    private final String name;
    private final long year;
    private final long lenght;
    private final long price;

    public Movie(String name, long year, long lenght, long price) {
        this.name = name;
        this.year = year;
        this.lenght = lenght;
        this.price = price;
    }

    public String getName() {
        return name;
    }

    public long getYear() {
    	return year;
    }
    
    public long getLenght() {
    	return lenght;
    }
    public long getPrice() {
        return price;
    }

    @Override
    public String toString() {
        return "Movie{" +
                "name='" + name + '\'' +
                ", year='" + year + '\'' +
                ", lenght='" + lenght + '\'' +
                ", price=" + price +
                '}';
    }
}
